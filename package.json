    "@signal-fire/server": "^0.5.0",
    "@types/ws": "^8.2.2",
    "cellx-collections": "^1.0.3",
    "cellx-decorators": "^1.6.3",
    "fastify": "3.11",
    "fastify-websocket": "^4.0.0",
