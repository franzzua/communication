{
  "name": "@franzzua/communication",
  "version": "1.0.5",
  "description": "",
  "main": "index.js",
  "scripts": {
    "all": "cmmn spawn compile --watch bundle -b --watch serve -b",
    "compile": "cmmn compile --watch",
    "web:bundle": "cmmn bundle main --watch",
    "serve": "cmmn serve -b",
    "web:prod": "cmmn bundle main --prod",
    "server:bundle": "cmmn bundle server",
    "server:run": "node dist/esm/server/server.js",
    "server:watch": "nodemon dist/bundle/server.js",
    "update": "npm version patch && git push",
    "ci": "cmmn compile -b && cmmn bundle main --prod && cmmn bundle server --prod && cp -r ./assets ./dist/bundle"
  },
  "files": [
    "dist/bundle"
  ],
  "type": "module",
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@cmmn/app": "1.7.17",
    "@cmmn/cell": "1.7.17",
    "@cmmn/core": "1.7.17",
    "@cmmn/domain": "1.7.17",
    "@cmmn/infr": "1.7.2",
    "@cmmn/server": "1.7.17",
    "@cmmn/sync": "1.7.17",
    "@cmmn/tools": "1.7.12",
    "@cmmn/ui": "1.7.17",
    "@inhauth/core": "^1.0.0",
    "@jest/globals": "latest",
    "@swc/core": "^1.3.0",
    "@swc/jest": "latest",
    "@testdeck/jest": "latest",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.18",
    "@types/simple-peer": "^9.11.4",
    "@types/ws": "^8.2.2",
    "is-mobile": "^3.0.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-node-exports-resolver": "^1.1.6",
    "jest-resolve": "^29.3.1",
    "jest-text-transformer": "latest",
    "jose": "^4.4.0",
    "jwt-decode": "^3.1.2",
    "less": "^4",
    "lib0": "0.2.47",
    "linkedom": "^0.14.21",
    "luxon": "2.4.*",
    "msgpackr": "^1.8.5",
    "node-fetch": "^3.2.10",
    "ts-jest": "latest",
    "typescript": "4.6.x",
    "typescript-transform-paths": "^3.3.1",
    "y-indexeddb": "9.x.x",
    "y-protocols": "1.x.x",
    "yjs": "13.x.x"
  },
  "cmmn": {
    "main": {
      "input": "dist/esm/entry/index.js",
      "module": "es",
      "html": "./entry/index.html",
      "dedupe": [
        "lib0"
      ],
      "external": [
        "@cmmn.*",
        "luxon",
        "msgpackr/pack"
      ],
      "alias": {
        "luxon": "luxon/build/es6/luxon.js"
      },
      "mount": {},
      "browser": true,
      "port": 4001
    },
    "server": {
      "input": "dist/esm/server/server.js",
      "module": "es",
      "external": [
        "fastify",
        "@fastify/.*"
      ]
    }
  },
  "peerDependenciesMeta": {
    "ts-node": {
      "optional": true
    },
    "postcss": {
      "optional": true
    }
  },
  "dependencies": {
    "@fastify/websocket": "^7.1.2",
    "fastify": "^3.29.4"
  }
}
