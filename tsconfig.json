{
  "extends": "@cmmn/tools/compile/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@model": [
        "./model"
      ],
      "@infr/*": [
        "./infr/*"
      ],
      "@services": [
        "./services"
      ],
      "@domain/*": [
        "./domain/*"
      ],
      "@domain": [
        "./domain"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "specs",
    "**/*.spec.ts",
    "dist"
  ]
}
